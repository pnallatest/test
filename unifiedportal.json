{
    "serviceProvider": "unifiedportal",
    "exemptions": {
      "allowWildcardUri": "SA-WILDCARDURI",
      "allowImplicitFlow": "SA-IMPLICITFLOW"
    },
    "environments": [
      {
        "env": "falcon",
        "falconInstance": "aws-dev1-uswest2",
        "userFederation": {
          "ldapRoleMappers": [
              {
                  "dn": "ou=appgroups,ou=unifiedportal,ou=tenants,ou=teams,ou=services,ou=infraservices,dc=falcondev,dc=sfdc,dc=net",
                  "cn": "unified-identity-portal-admin"
              }
          ]
        },
        "accessTokenLifespan": 72000,
        "refreshTokenLifespan": 72000,
        "clientsOIDC": [
          {
            "clientId": "unifiedportal-OIDC",
            "redirectUris": [
              "https://localhost:3030/oauth2",
              "https://oidcdebugger.com/debug"
            ],
            "secretsReaders": [
              "arn:aws:iam::391733816673:role/PCSKDeveloperRole",
              "arn:aws:iam::391733816673:role/PCSKAdministratorAccessRole"
            ],
            "consentRequired": false,
            "standardFlowEnabled": true,
            "implicitFlowEnabled": false,
            "directAccessGrantsEnabled": false,
            "clientSecretEnabled": true
          },
          {
            "clientId": "unifiedportal-pkce",
            "redirectUris": [
              "https://localhost:3030/*",
              "https://oidcdebugger.com/debug"
            ],
            "consentRequired": false,
            "codeChallengeMethod": "S256",
            "clientSecretEnabled": false,
            "directAccessGrantsEnabled": false,
            "implicitFlowEnabled": false,
            "standardFlowEnabled": true
          }
        ],
        "clientsSaml": []
      }
    ]
  }